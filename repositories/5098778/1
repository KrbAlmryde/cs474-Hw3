+  public static void getMinDate(long idPlace)
+  {
+    Place place = Place.findById(idPlace);
+    Info info = Info.find("byPlace order by startDate", place).first();
+    Date minDate = info.startDate;
+    renderText(minDate.getTime());
+  }
