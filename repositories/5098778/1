+  public static void getMaxDate(long idPlace)
+  {
+    Place place = Place.findById(idPlace);
+    Info info = Info.find("byPlace order by startDate desc", place).first();
+    Date maxDate = info.startDate;
+    renderText(maxDate.getTime());
+  }
