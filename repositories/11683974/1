+                    msg = ((Boolean) result) ? srcActivity.getString(R.string.load_ok) : ser.getErrString();
+                }
+                retValue = ((Boolean) result) ? 0 : -1;
+            } else if (connectionPart) {
+                msg = con.getResponseStr();
+                retValue = con.getResponseCode();
